{
  "manifest_version": 2,
  "name": "IEEE Liberator",
  "description": "Take back control over research papers",
  "version": "1.0",
  "browser_action": {
    "default_icon": "scihub.jpg"
  },
  "content_scripts": [
    {
      "matches": [
        "https://ieeexplore.ieee.org/document/*"
      ],
      "js": [
        "extension.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ]
}